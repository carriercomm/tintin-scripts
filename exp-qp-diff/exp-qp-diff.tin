#action {^You have scored %d experience points and %d quest points.$} {#var oldexp $newexp;#var newexp %1;#math exp {$newexp - $oldexp};#var oldqp $newqp;#var newqp %2;#math qp {$newqp - $oldqp}} {4}
#sub {^You have scored %d experience points and %d quest points.$} {You have scored %1 experience points and %2 quest points. ($exp/$qp)} {5}
#alias exp {#var exp}
#alias qp {#var qp}
